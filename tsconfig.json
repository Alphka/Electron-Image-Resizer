{
	"compilerOptions": {
		"jsx": "react-jsx",
		"target": "ESNext",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"allowJs": true,
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
		],
		"baseUrl": ".",
		"outDir": "dist"
	}
}
